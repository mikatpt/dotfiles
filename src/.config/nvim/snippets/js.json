{
	"console.log": {
		"prefix": "clg",
		"body": [
			"console.log($0)"
		],
		"description": "Log output to console"
	},
	"console.error": {
		"prefix": "cle",
		"body": [
			"console.error($0)"
		],
		"description": "Log output to console"
	},
	"console.time": {
		"prefix": "clt",
		"body": [
			"console.time$0()"
		],
		"description": "Start a timer. Run console.timeEnd() to stop the timer."
	},
	"log.stringify": {
		"prefix": "clj",
		"body": [
			"console.log(JSON.stringify($0, null, 2))"
		],
		"description": "Log formatted and stringified output to console"
	},
	"jest test suite": {
		"prefix": "des",
		"body": [
			"describe('$1', () => {\n\ttest('$2', () => {\n\t\tconst res = $3;\n\t\texpect(res).toBe($4);\n\t});\n});"
		],
		"description": "Create Jest test suite."
	},
	"jest test": {
		"prefix": "tes",
		"body": [
			"test('$1', () => {\n\tconst res = $2;\n\texpect(res).toBe($3);\n});"
		],
		"description": "Create single Jest test."
    }
}
